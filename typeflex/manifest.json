{
    "name": "Typeflex",
    "version": "1.0.0",
    "description": "text replacer",
    "manifest_version": 3,
    "author": "Aryan Verma",
    "action":{
        "default_popup": "index.html"
    },

    "permissions": ["activeTab"],

    "icons": {
        "16": "logo-16.png",
        "32": "logo-32.png",
        "48": "logo-48.png",
        "128": "logo-128.png"
    },

    "content_scripts":[
        {
            "matches": ["<all_urls>"], 
            "js" : ["temp.js"]
        }
    ]

}